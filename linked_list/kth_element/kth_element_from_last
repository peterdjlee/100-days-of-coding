Kth element from last element 

kToIndex(int k, int length)
size(Node* head)
nodeAt(int index, Node* head)

kthElement(int k, Node* head):
int index = kToIndex(k, size(head))
Return nodeAt(index, head)

kToIndex(int k, int length):
Return length - 1 - k

size(Node* head):
len = 1
t = head
While t->getNext != nullptr:
Len++
Return len

nodeAt(int index, Node* head):
t = head
I = 0
While i < index:
T= t->getNext()
i++
Return t
